{
  "name": "universal-react-mobx",
  "license": "ISC",
  "version": "0.13.1",
  "author": "LukeLin",
  "keywords": [
    "react",
    "mobx",
    "webpack2",
    "babel",
    "isomophic react",
    "universal react"
  ],
  "main": "./dist/server.js",
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-async-to-generator",
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "fastclick": "1.0.6",
    "helmet": "^3.5.0",
    "mobx": "^3.1.7",
    "mobx-react": "^4.1.3",
    "mobx-react-form": "^1.31.1",
    "morgan": "^1.8.1",
    "pm2": "^2.4.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0",
    "secure-filters": "^1.0.5",
    "source-map-support": "^0.4.14",
    "validatorjs": "^3.12.0"
  },
  "description": "universal-react-mobx-starter",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "browser-sync": "^2.18.8",
    "cross-env": "^4.0.0",
    "css-loader": "^0.27.3",
    "del": "^2.2.2",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "gulp": "^3.9.1",
    "happypack": "^3.0.3",
    "json-loader": "^0.5.4",
    "mobx-react-devtools": "^4.2.11",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "react-addons-perf": "^15.4.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "why-did-you-update": "0.0.8"
  },
  "scripts": {
    "start": "node starter.js",
    "kill": "pm2 delete universal-react-mobx",
    "build:lib": "gulp build:lib:dev && gulp build:lib",
    "build:dev": "cross-env NODE_ENV=development && gulp build:dev",
    "build": "npm run build:prod && npm run build:dev",
    "build:prod": "cross-env NODE_ENV=production && gulp build",
    "build:server": "webpack --progress --config ./webpack.config-server.js",
    "local": "npm run build:server && nodemon index.js",
    "debug": "npm run build:server && node-debug index.js",
    "watch": "gulp watch"
  },
  "engines": {
    "node": ">=7.0.0"
  }
}
