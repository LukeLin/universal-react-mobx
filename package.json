{
  "name": "universal-react-mobx",
  "license": "ISC",
  "version": "0.13.1",
  "author": "LukeLin",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel",
    "immutable",
    "isomophic react",
    "universal react"
  ],
  "main": "./dist/server.js",
  "babel": {
    "presets": [
      "react"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-async-to-generator",
      "add-module-exports",
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "ejs": "2.5.2",
    "express": "4.14.0",
    "express-session": "1.14.1",
    "fastclick": "1.0.6",
    "helmet": "^2.3.0",
    "mobx": "^2.6.3",
    "mobx-react": "^3.5.9",
    "morgan": "1.7.0",
    "pm2": "^2.0.18",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "source-map-support": "^0.4.5"
  },
  "description": "isomophic-react-redux-starter",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-runtime": "^6.18.0",
    "browser-sync": "^2.17.5",
    "css-loader": "^0.23.1",
    "del": "^2.2.2",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "happypack": "^2.2.1",
    "json-loader": "^0.5.4",
    "mobx-react-devtools": "^4.2.10",
    "nodemon": "^1.11.0",
    "react-addons-perf": "^15.4.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "why-did-you-update": "0.0.8"
  },
  "scripts": {
    "start": "node starter.js",
    "kill": "pm2 delete universal-react-mobx",
    "build:lib": "gulp build:lib:dev && gulp build:lib",
    "build:dev": "set NODE_ENV=development && gulp build:dev",
    "build": "npm run build:prod && npm run build:dev",
    "build:prod": "set NODE_ENV=production && gulp build",
    "build:server": "webpack --progress --config ./webpack.config-server.js",
    "local": "npm run build:server && nodemon index.js",
    "debug": "npm run build:server && node-debug index.js",
    "watch": "gulp watch"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
